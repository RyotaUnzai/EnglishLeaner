* {
    font-family: Verdana, Geneva, Tahoma, sans-serif;
    color: #fff;
    background-color: #333;
}

QLineEdit {
    border: solid 1px;
    background-color: #222222;
}

QPushButton {
    max-width: 24px;
    background-color: #00000000;
}
QPushButton:hover {
    max-width: 24px;
    background-color: #111;
}

QTextEdit {
    background-color: #00000000;
    border: solid 0px;
}

QRadioButton {
    color: #fff;
}

QLabel#labelResult {
    color:#fff;
    font-weight: bold;
}
